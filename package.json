{
  "name": "ankeny-chiropractors",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "set NODE_ENV=production && npm run build:vue && npm run copy",
    "lint": "vue-cli-service lint",
    "build:vue": "vue-cli-service build",
    "copy": "npm run copy:robots && npm run copy:sitemap && npm run copy:redirects",
    "copy:robots": "cp ./robots.txt ./dist",
    "copy:redirects": "cp ./_redirects ./dist",
    "copy:sitemap": "cp ./sitemap.xml ./dist"
  },
  "dependencies": {
    "@tailwindcss/postcss7-compat": "^2.2.17",
    "@vuelidate/core": "^2.0.0-alpha.40",
    "@vuelidate/validators": "^2.0.0-alpha.28",
    "ajv-keywords": "^5.1.0",
    "autoprefixer": "^9",
    "postcss": "^7",
    "prerender-spa-plugin": "^3.4.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17",
    "vue": "^3.0.0",
    "vue-3-sanitize": "^0.1.4",
    "vue-meta": "^3.0.0-alpha.7",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "ajv-keywords": "^5.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.0.0",
    "prettier": "^2.2.1",
    "vue-cli-plugin-tailwind": "~2.2.18"
  }
}
